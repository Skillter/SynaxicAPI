# Synaxic API Environment Variables
# Copy this file to .env and fill in your actual values
# do NOT commit the .env file to Git

# --- Google OAuth2 ---
GOOGLE_CLIENT_ID=your-google-client-id-goes-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-goes-here

# --- Production Database ---
POSTGRES_USER=synaxic
POSTGRES_PASSWORD=generate-a-strong-password-here
POSTGRES_DB=synaxic

# --- Grafana Credentials ---
GRAFANA_ADMIN_PASSWORD=generate-a-strong-password-here